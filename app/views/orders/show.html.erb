<center>

<table> 
  <th>Title</th>
  <th>Quantity</th>
  <th>Total_price</th>
  <th colspan=3>Actions</th>
  <tr>  
    <td><%= @order.product.title %></td>
    <td><%= @order.quantity %></td>
    <td><%= @order.total_price %></td>
    <td><%= button_to "Edit", edit_product_order_path(@product,@order),method: :get %></td>
    <td><%= button_to "Delete", product_order_path(@product,@order),method: :delete %></td>
    <td><%= button_to "Back", product_orders_path(@product),method: :get %></td>
  </tr>
</table>

</center>
